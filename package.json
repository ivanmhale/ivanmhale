{
  "name": "homebase",
  "version": "1.0.0",
  "description": "website",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server  --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "watch:sass": "node-sass styles/sass/index.scss dist/index.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist/index.css -o styles/index.prefix.css",
    "compress:css": "node-sass styles/index.prefix.css dist/index.css --output-style compressed",
    "build:css": "npm-run-all prefix:css compress:css"
  },
  "author": "ivanmhale",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-cli": "^6.1.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "clean-webpack-plugin": "^3.0.0"
  }
}
