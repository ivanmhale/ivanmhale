{
  "name": "homebase",
  "version": "1.0.0",
  "description": "website",
  "main": "index.js",
  "scripts": {
    "start": "live-server dist/index.html -w",
    "watch:sass": "node-sass styles/sass/index.scss dist/index.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' dist/index.css -o styles/index.prefix.css",
    "compress:css": "node-sass styles/index.prefix.css dist/index.css --output-style compressed",
    "build:css": "npm-run-all prefix:css compress:css"
  },
  "author": "ivanmhale",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^9.5.1",
    "live-server": "^1.2.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {}
}
